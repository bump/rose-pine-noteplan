{
  "name": "RosÃ© Pine Dawn",
  "isDark": false,
  "editor": {
    "backgroundColor": "#faf4ed",
    "altBackgroundColor": "#fffaf3",
    "tintColor": "#907aa9",
    "tintColor2": "#56949f",
    "textColor": "#575279",
    "toolbarBackgroundColor": "#f2e9e1",
    "toolbarIconColor": "#797593",
    "menuItemColor": "#797593",
    "timeBlockColor": "#d7827e",
    "shouldOverwriteFont": false
  },
  
  "__orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "checked-todo-characters",
    "todo",
    "checked",
    "quote-mark",
    "tabbed",
    "link",
    "hashtag",
    "attag",
    "schedule-to-date-link",
    "done-date",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3"
  ],

  "styles": {
    "body": {
      "color": "#575279",
      "font": "AvenirNext-Regular",
      "size": 16
    },
    "title1": {
      "font": "AvenirNext-Bold",
      "size": 28,
      "color": "#907aa9"
    },
    "title2": {
      "font": "AvenirNext-Bold",
      "size": 24,
      "color": "#56949f"
    },
    "title3": {
      "font": "AvenirNext-Bold",
      "size": 20,
      "color": "#d7827e"
    },
    "bold": {
      "font": "AvenirNext-Bold",
      "color": "#575279"
    },
    "italic": {
      "font": "AvenirNext-Italic",
      "color": "#575279"
    },
    "todo": {
      "color": "#575279",
      "headIndent": 33,
      "size": 16,
      "lineSpacing": 0.90
    },
    "checked-todo-characters": {
      "headIndent": 33,
      "size": 16
    },
    "tabbed": {
      "headIndent": 43
    },
    "checked": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\])([^@]*)",
      "color": "#9893a5",
      "matchPosition": 0,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#669893a5",
      "strikethroughStyle": 1,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 0,
      "color": "#4456949f",
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "quote-mark": {
      "color": "#9893a5",
      "headIndent": 33
    },
    "quote-content": {
      "color": "#6e6a86",
      "type": "italic",
      "headIndent": 33
    },
    "link": {
      "color": "#907aa9"
    },
    "schedule-to-date-link": {
      "color": "#d7827e"
    },
    "done-date": {
      "color": "#9893a5"
    },
    "schedule-from-date-link": {
      "color": "#d7827e"
    },
    "hashtag": {
      "color": "#56949f",
      "backgroundColor": "#fffaf3",
      "borderRadius": 6
    },
    "attag": {
      "color": "#ea9d34",
      "backgroundColor": "#fffaf3",
      "borderRadius": 6
    },
    "highlighted": {
      "backgroundColor": "#f4ede8",
      "color": "#575279"
    },
    "strikethrough": {
      "color": "#9893a5",
      "strikethroughColor": "#9893a5"
    },
    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#ea9d34"
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#286983",
      "backgroundColor": "#fffaf3",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#66286983"
    },
    "code-right-backtick": {
      "color": "#66286983"
    },
    "code-fence": {
      "theme": "github",
      "corner-radius": 8,
      "body": {
        "font": "Menlo-Regular",
        "backgroundColor": "#fffaf3",
        "size": 14
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "color": "#9893a5",
        "size": 14
      },
      "fence-open": {
        "color": "#9893a5"
      },
      "fence-close": {
        "color": "#9893a5"
      }
    },
    "flagged-1": {
      "backgroundColor": "#d7827e20",
      "color": "#d7827e"
    },
    "flagged-2": {
      "backgroundColor": "#d7827e30",
      "color": "#d7827e"
    },
    "flagged-3": {
      "backgroundColor": "#d7827e40",
      "color": "#d7827e"
    },
    "working-on": {
      "backgroundColor": "#907aa920",
      "color": "#907aa9"
    }
  }
}