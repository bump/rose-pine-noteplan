{
  "name": "RosÃ© Pine Moon",
  "isDark": true,
  "editor": {
    "backgroundColor": "#232136",
    "style": "Dark",
    "sidebarStyleOverride": "dark",
    "sidebarIconColorOverride": "#c4a7e7",
    "altBackgroundColor": "#2a273f",
    "tintColor": "#c4a7e7",
    "tintColor2": "#9ccfd8",
    "textColor": "#e0def4",
    "toolbarBackgroundColor": "#393552",
    "toolbarIconColor": "#908caa",
    "menuItemColor": "#908caa",
    "timeBlockColor": "#eb6f92",
    "shouldOverwriteFont": false
  },
  
  "__orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "checked-todo-characters",
    "todo",
    "checked",
    "quote-mark",
    "tabbed",
    "link",
    "hashtag",
    "attag",
    "schedule-to-date-link",
    "done-date",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3"
  ],

  "styles": {
    "body": {
      "color": "#e0def4",
      "font": "AvenirNext-Regular",
      "size": 16
    },
    "title1": {
      "font": "AvenirNext-Bold",
      "size": 28,
      "color": "#c4a7e7"
    },
    "title2": {
      "font": "AvenirNext-Bold",
      "size": 24,
      "color": "#9ccfd8"
    },
    "title3": {
      "font": "AvenirNext-Bold",
      "size": 20,
      "color": "#ea9a97"
    },
    "bold": {
      "font": "AvenirNext-Bold",
      "color": "#e0def4"
    },
    "italic": {
      "font": "AvenirNext-Italic",
      "color": "#e0def4"
    },
    "todo": {
      "color": "#e0def4",
      "headIndent": 33,
      "size": 16,
      "lineSpacing": 0.90
    },
    "checked-todo-characters": {
      "headIndent": 33,
      "size": 16
    },
    "tabbed": {
      "headIndent": 43
    },
    "checked": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\])([^@]*)",
      "color": "#6e6a86",
      "matchPosition": 0,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#666e6a86",
      "strikethroughStyle": 1,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 0,
      "color": "#449ccfd8",
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "quote-mark": {
      "color": "#6e6a86",
      "headIndent": 33
    },
    "quote-content": {
      "color": "#908caa",
      "type": "italic",
      "headIndent": 33
    },
    "link": {
      "color": "#c4a7e7"
    },
    "schedule-to-date-link": {
      "color": "#ea9a97"
    },
    "done-date": {
      "color": "#6e6a86"
    },
    "schedule-from-date-link": {
      "color": "#ea9a97"
    },
    "hashtag": {
      "color": "#9ccfd8",
      "backgroundColor": "#2a273f",
      "borderRadius": 6
    },
    "attag": {
      "color": "#f6c177",
      "backgroundColor": "#2a273f",
      "borderRadius": 6
    },
    "highlighted": {
      "backgroundColor": "#393552",
      "color": "#e0def4"
    },
    "strikethrough": {
      "color": "#6e6a86",
      "strikethroughColor": "#6e6a86"
    },
    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#f6c177"
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#3e8fb0",
      "backgroundColor": "#2a273f",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#663e8fb0"
    },
    "code-right-backtick": {
      "color": "#663e8fb0"
    },
    "code-fence": {
      "theme": "nord",
      "corner-radius": 8,
      "body": {
        "font": "Menlo-Regular",
        "backgroundColor": "#2a273f",
        "size": 14
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "color": "#6e6a86",
        "size": 14
      },
      "fence-open": {
        "color": "#6e6a86"
      },
      "fence-close": {
        "color": "#6e6a86"
      }
    },
    "flagged-1": {
      "backgroundColor": "#eb6f9220",
      "color": "#eb6f92"
    },
    "flagged-2": {
      "backgroundColor": "#eb6f9230",
      "color": "#eb6f92"
    },
    "flagged-3": {
      "backgroundColor": "#eb6f9240",
      "color": "#eb6f92"
    },
    "working-on": {
      "backgroundColor": "#c4a7e720",
      "color": "#c4a7e7"
    }
  }
}